---
import type { ImageMetadata } from "astro";
import { Image } from "astro:assets";

interface Props {
  images: ImageMetadata[];
}
const { images } = Astro.props;
---

<div class="flex flex-col gap-12 p-4 md:flex-row">
  {
    images.map((image) => (
      <Image
        src={image}
        alt={"Wall Print Sample"}
        class="rounded-md w-[230px] h-[230px]"
      />
    ))
  }
</div>
